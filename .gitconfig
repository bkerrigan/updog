[user]
  name = Bryan Kerrigan
  email = bryankerrigan@gmail.com
[github]
  username = bkerrigan
  token = 5b3e8706f4c8ce4f1981e73595d7a627

[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  ssexcludesfile = /Users/bkerrigan/.gitignore
  sseditor = /usr/bin/vim

[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  dc = diff --cached
  history = log
  lg = log -p
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[push]
  ssdefault = current
  default = current
[rebase]
  ssautosquash = true
[grep]
  sslineNumber = true
  ssextendRegexp = true
[help]
  autocorrect = 1

